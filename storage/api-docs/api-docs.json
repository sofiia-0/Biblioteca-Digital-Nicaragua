    "paths": {
        "/api/libros": {
            "get": {
                "tags": [
                    "Libros"
                ],
                "summary": "Listar libros",
                "operationId": "08f70de053dbd61674113fd6369fe329",
                "responses": {
                    "200": {
                        "description": "Listado de libros"
                    }
