            "post": {
                "tags": [
                    "Libros"
                ],
                "summary": "Crear libro",
                "operationId": "c4bb407519a52e2207d4fe944d74b80e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "titulo",
                                    "autor"
                                ],
                                "properties": {
                                    "titulo": {
                                        "type": "string",
                                        "example": "Cien años de soledad"
                                    },
                                    "autor": {
                                        "type": "string",
                                        "example": "Gabriel García Márquez"
                                    },
                                    "anio": {
                                        "type": "integer",
                                        "example": 1967
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Libro creado"
                    },
                    "422": {
                        "description": "Datos inválidos"
                    }
                }
            }
        },
